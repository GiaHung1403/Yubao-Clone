{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-var-requires": false,
    "prettier": true
  },
  "defaultSeverity": "error",
  "rulesDirectory": []
}